#if !IO
#define IO

fn inb(port: u16) u8 {
    var result: u8; // ebp-4
    asm("movw 8(%ebp), %dx"); // port
    asm("inb %dx, %al");
    asm("movb %al, -4(%ebp)");
    return result;
}

fn outb(value: u8, port: u16) void {
    asm("movb 8(%ebp), %al"); // value
    asm("movw 12(%ebp), %dx"); // port
    asm("outb %al, %dx");
}

#endif
